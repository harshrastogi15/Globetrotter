{
    "version": 2,
    "builds": [
      {
        "src": "./src/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/dist",
        "use": "@vercel/static"
      }
    ],

    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "server.js"
        }
    ]
}
